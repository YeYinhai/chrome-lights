{
	"name": "百度灯火",
	"manifest_version": 2,
	"version": "2.6.0",
	"description": "过滤百度无用信息",
	"page_action": {
		"default_icon": "img/icon.png",
		"default_title": "百度灯火",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"declarativeContent",
		"storage"
	],
	"background": {
		"scripts": [
			"js/background.js"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"*://www.baidu.com/*"
			],
			"js": [
				"js/jquery-3.3.1.min.js",
				"js/content.js"
			],
			"css": [
				"css/content.css"
			],
			"run_at": "document_end"
		}
	],
	"homepage_url": "https://github.com/YeYinhai"
}